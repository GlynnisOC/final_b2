<h1>All Courses Offered</h1>

<% @courses.each do |course| %>
<section id="course-<%= course.id %>">
  <h4><%= course.name %></h4>
  <% course.course_students.stars_to_suckers.each do |course_student| %>
    <%= course_student.student.name %>
    <%= course_student.grade %><br>
  <% end %>
</section>
<% end %>
